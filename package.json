{
  "name": "cindy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css": "postcss assets/styles/main.css -o assets/styles/main.min.css --verbose --env 'production'",
    "server-reload": "browser-sync start --config './browser-sync-config.js'",
    "develop": "concurrently 'npm:css -- --watch' 'npm:server-reload'",
    "build:css": "npm run css -- --env 'production'",
    "format": "npx prettier ./**/*.html",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepreview": "npm run build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore src"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@tinymce/tinymce-vue": "^5.1.1",
    "@unhead/vue": "^1.8.9",
    "axios": "^0.21.4",
    "bootstrap": "^5.3.2",
    "express": "^4.18.2",
    "lightgallery": "^2.7.2",
    "luxon": "^3.4.4",
    "postcss-cli": "^9.1.0",
    "swiper": "^11.0.5",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.42.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^7.20.0",
    "postcss": "^8.4.29",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.3.3",
    "vite": "^3.2.7",
    "vite-plugin-require-transform": "^1.0.21",
    "vite-plugin-vue-images": "^0.6.1",
    "vue-eslint-parser": "^9.3.0"
  }
}
